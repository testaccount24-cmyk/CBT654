%===>_ZCMD                                                             +
)END
./ ADD NAME=SAMP01A  8014-74296-75281-1043-00032-00024-00000-TCS3
)ATTR DEFAULT(%+_)
 ! TYPE(OUTPUT) INTENS(HIGH)
 : TYPE(INPUT)  INTENS(NON)
)BODY
+
+                                 +TEST PANEL
+
+
+
+               %@@@@@@@@@@@@@@     @@@@@@@@@@@    @@@@@@@@@@@@
+              %@@@@@@@@@@@@@@    @@@@@@@@@@@@    @@@@@@@@@@@@
+             %@@@@              @@@@                @@@@
+            %@@@@@@@@@@         @@@@@@@@@@         @@@@
+           %@@@@@@@@@@                @@@@        @@@@
+          %@@@@              @@@@@@@@@@@@    @@@@@@@@@@@@
+         %@@@@              @@@@@@@@@@@     @@@@@@@@@@@@
+
+       !Z       +
+                           +USERID%===>_Z       +
+
+                           +PASSWD%===>:Z       +
+
+
+
+
+
!HITS    %HITS+
%===>_ZCMD                                                             +
)PROC
 .ZVARS = '(ZSTAT USERID
                  PASSWD)'
)END
./ ADD NAME=SAMP01B  8007-74299-75281-1626-00028-00029-00000-TCS3
)ATTR DEFAULT(%+_)
 ! TYPE(OUTPUT) INTENS(HIGH)
)BODY
+
+
+
+
+
+
+
+
+
+ WELCOME TO FSI TEST SYSTEM
+
+     PLEASE ENTER A COMMAND TO CONTINUE
+
+     CURRENTLY THE ONLY%VALID+COMMANDS ARE:
+         %END+
+
+
+
+
+
+
+
!HITS    %HITS+
%===>_ZCMD                                                             +
)END
./ ADD NAME=SAMP02A  8011-74304-74304-1550-00028-00027-00000-TCS3
)ATTR DEFAULT(%+:)
 # TYPE(OUTPUT) INTENS(LOW)
)BODY
%--------------------------------- FSI MAIN MENU ------------------------------
%OPTION ===>:ZCMD                                                              +
+                                                            USERID  -#ZUSER   +
   %0 +DEFAULTS    - ALTER / DISPLAY SESSION DEFAULS         TIME    -#ZTIME   +
   %1 +VIEW        - DISPLAY SOURCE DATA OR OUTPUT LISTINGS  SYSTEM  -#ZSYS    +
   %2 +EDIT        - UPDATE / CREATE A MEMBER OR DATASET     TSO-PROC-#ZPROC   +
   %3 +UTILITY     - ENTER UTILITY
   %4 +ASSEMBLER   - FOREGROUND ASSEMBLER AND LINK
   %5 +USER        - EXECUTE RPF USER ROUTINE
   %6 +TSO         - EXECUTE TSO COMMANDS
   %7 +TUTORIAL    - DISPLAY HELP INFORMATION
   %8 +TEST        - ENTER TEST MODE (AUTHORIZED)
   %9 +OPERATOR    - ENTER OPERATOR MODE
   %X +EXIT        - TERMINATE FSI
+
+HIT%PF03/15+TO TERMINATE FSI
% ____________________________
%|                            |
%| FSI VERSION 1 RELEASE 1.0  |
%|                            |
%| WRITTEN BY TOMMY SPRINKLE  |
%| TOMMY@TOMMYSPRINKLE.COM    |
%|____________________________|                                        +
+
)END
./ ADD NAME=SAMP03A  8001-75302-75302-1815-00028-00028-00000-TCS3
)ATTR DEFAULT(%+:)
 # TYPE(OUTPUT) INTENS(LOW)
)BODY
%--------------------------------- FSI TEST PANEL -----------------------------
%OPTION ===>:ZCMD                                                              +
+
+  +A NUMERIC FIELD%==>:NUMBER+
+
+  +THE NUMERIC FIELD (#NUMBER+)
+
+
+
+
+
+
+
+
+
+HIT%PF03/15+TO TERMINATE FSI
+
+
+
+
+
+
+
+
)END
