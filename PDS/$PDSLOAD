//SBGOLOBL JOB (ACCT#),PDSLOAD,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//*
//*   NOTE..  THE LOADT MEMBER IS IN REVIEW OFFLOAD FORMAT,
//*           AND IT HAS TO BE RECONSTITUTED USING GREG PRICE'S
//*           REVIEW PROGRAM FROM CBT TAPE FILE 134.
//*
//*           THIS JOB RECONSTITUTES THE SOURCE-FORMAT FSI LIBRARIES.
//*
//PDSLOAD PROC IPDS='SBGOLOB.CBT463.FILE654',
//             OPRE=SBGOLOB,
//             QUAL=FILE654,
//             MEMB=XXX,
//             OUNT=SYSALLDA,
//             OVL=DATA04,
//             DSP1=NEW,
//             DSP2=CATLG,
//             DSP3='',
//             P=15,S=30,D=44
//*
//LOAD   EXEC PGM=PDSLOAD
//STEPLIB  DD DISP=SHR,DSN=CBT.CBT462.FILE035.PDS
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DISP=SHR,DSN=&IPDS(&MEMB)
//SYSUT2   DD UNIT=&OUNT,VOL=SER=&OVL,
//             DSN=&OPRE..&QUAL..&MEMB,
//             DISP=(&DSP1,&DSP2&DSP3),
//             SPACE=(TRK,(&P,&S,&D),RLSE)
//  PEND
//*
//S001 EXEC PDSLOAD,MEMB=ASMT
//S002 EXEC PDSLOAD,MEMB=MLIBT
//S003 EXEC PDSLOAD,MEMB=OBJT
//S004 EXEC PDSLOAD,MEMB=PLIBT
